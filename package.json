{
  "name": "super-clippy",
  "description": "Webcomponent super-clippy following open-wc recommendations",
  "license": "MIT",
  "author": "super-clippy",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./super-clippy.js": "./dist/src/super-clippy.js"
  },
  "scripts": {
    "analyze": "cem analyze",
    "start": "npm run api && npm run bundle && tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "bundle": "rollup -c rollup.config.js",
    "build": "npm run api && npm run bundle && tsc && npm run analyze -- --exclude dist",
    "prepublish": "tsc && npm run analyze -- --exclude dist",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "test": "tsc && wtr --coverage",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
    "storybook": "tsc && npm run analyze -- --exclude dist && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
    "storybook:build": "tsc && npm run analyze -- --exclude dist && build-storybook",
    "proxy": "npx spandx -c spandx.config.cjs",
    "api": "node scripts/api-key-generate.js"
  },
  "dependencies": {
    "@cpelements/pfe-navigation": "^1.0.125",
    "@patternfly/elements": "^2.4.0",
    "@patternfly/pfe-accordion": "^1.12.3",
    "@patternfly/pfe-avatar": "^1.12.3",
    "@patternfly/pfe-content-set": "^1.12.3",
    "@patternfly/pfe-icon": "^1.12.3",
    "@patternfly/pfe-jump-links": "^1.12.3",
    "@rhds/elements": "^1.2.0",
    "@shoelace-style/shoelace": "^2.11.2",
    "lit": "2.6.1",
    "web-dev-server-plugin-lit-css": "^2.0.5"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@open-wc/eslint-config": "^9.2.1",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-storybook": "^0.5.4",
    "@web/test-runner": "^0.14.0",
    "concurrently": "^5.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-import-css": "^3.3.1",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-terser": "^7.0.2",
    "spandx": "^2.2.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "customElements": "custom-elements.json",
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@open-wc",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "import/no-unresolved": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "ignorePackages": true
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
